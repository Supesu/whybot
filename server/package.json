{
  "name": "whybot",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "gen-env": "gen-env-types .env -o src/env.d.ts -e .",
    "clean": "rimraf -rf build && rimraf ./src/env.d.ts",
    "build:ts": "tsc",
    "build": "npm run build:ts",
    "watch": "concurrently -k -p \"{name}\" -n \",\" -c \"cyan.bold, green.bold\" \"npm run watch:ts\" \"npm run watch:serve\"",
    "watch:ts": "tsc -w",
    "watch:serve": "nodemon -r dotenv-safe/config build/index.js",
    "start": "node build/index.js"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^17.0.16",
    "@types/tmi.js": "^1.8.1",
    "concurrently": "^7.0.0",
    "dotenv-safe": "^8.2.0",
    "gen-env-types": "^1.3.2",
    "nodemon": "^2.0.15",
    "rimraf": "^3.0.2",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "chalk": "^4.1.1",
    "discord.js": "^13.6.0",
    "moment": "^2.29.1",
    "tmi.js": "^1.8.5"
  }
}
